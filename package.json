{
  "name": "@stardust-js/ui",
  "version": "1.0.110",
  "description": "Everything I want to publish here",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host=0.0.0.0 --port=3006 --open",
    "build": "vite build --config vite.lib.config.js",
    "serve": "vite preview",
    "push": "git add . && git commit -m \"add\" && git push origin master",
    "pull": "git status && git pull origin master",
    "force-pull": "git status && git checkout . && git pull origin master",
    "cloc": "cloc vite.config.js src packages",
    "cloc-plop": "cloc plopfile.js .plop"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Stardust1001/stardust-browser.git"
  },
  "keywords": [
    "stardust",
    "stardust-ui"
  ],
  "author": "Stardust",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stardust1001/stardust-browser/issues"
  },
  "homepage": "https://github.com/Stardust1001/stardust-browser#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@stardust-js/browser": "latest",
    "@stardust-js/js": "latest",
    "@vant/area-data": "^1.5.0",
    "element-plus": "^2.4.1",
    "vant": "^4.7.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/cli": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "consola": "^3.1.0",
    "rollup-plugin-external-globals": "^0.7.1",
    "sass": "^1.69.0",
    "terser": "^5.16.4",
    "vite": "^4.4.11",
    "vite-plugin-style-import": "^2.0.0"
  }
}
