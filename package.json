{
  "name": "@stardust-js/ui",
  "version": "1.2.91",
  "description": "Everything I want to publish here",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host=0.0.0.0 --port=3006 --open",
    "build": "vite build --config vite.lib.config.js",
    "serve": "vite preview",
    "push": "git add . && git commit -m \"add\" && git push origin master",
    "pull": "git status && git pull origin master",
    "force-pull": "git status && git checkout . && git pull origin master",
    "cloc": "cloc vite.config.js src packages",
    "cloc-plop": "cloc plopfile.js .plop"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Stardust1001/stardust-ui.git"
  },
  "keywords": [
    "stardust",
    "stardust-ui"
  ],
  "author": "Stardust",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stardust1001/stardust-ui/issues"
  },
  "homepage": "https://github.com/Stardust1001/stardust-ui#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/cli": "^5.0.8",
    "@vue/compiler-sfc": "^3.4.13",
    "consola": "^3.2.3",
    "rollup-plugin-external-globals": "^0.9.1",
    "sass": "^1.69.7",
    "terser": "^5.26.0",
    "vite": "^5.0.11",
    "vite-plugin-style-import": "^2.0.0"
  },
  "dependencies": {
    "@vant/area-data": "^1.5.1",
    "el-table-infinite-scroll": "^3.0.3",
    "element-plus": "^2.5.1",
    "vant": "^4.8.2",
    "vue": "^3.4.13"
  }
}
